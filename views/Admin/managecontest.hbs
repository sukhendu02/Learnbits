    .modal-window {
        position: fixed;
        background-color: rgba(255, 255, 255, 0.25);
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        z-index: 999;
        visibility: hidden;
        opacity: 0;
        pointer-events: none;
        transition: all 0.3s;
      }
      .modal-window:target {
        visibility: visible;
        opacity: 1;
        pointer-events: auto;
      }
      .modal-window > div {
        width: 400px;
        position: absolute;
        top: 100px;
        left: 50%;
        transform: translate(-50%, -50%);
        padding: 2em;
        background: #617CEA;
        color: #fff;
      }
      .modal-window header {
        font-weight: bold;
